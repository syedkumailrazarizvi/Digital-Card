    const rsvpContacts = `RSVP Contacts:
1- SYED JAFFAR RAZA RIZVI +923340008988
2- SYED MUSTAFA RAZA +923340007765`;

    // 20:00 PKT => 15:00 UTC; 23:00 PKT => 18:00 UTC
    const startUTC = "20260216T150000Z";
    const endUTC   = "20260216T180000Z";

    const details =
`With the blessings of the Holy Ahlulbayt (Ø¹)
Mr & Mrs Syed Jaffar Raza Rizvi invite you to the marriage of their beloved son.

Schedule:
â€¢ Gathering at 7:00 PM
â€¢ Dinner at 8:30 PM

Qur'an 78:8 â€” "And We created you in pairs."

Please recite Surah Fatiha for our marhumeen.

${rsvpContacts}

Location: ${mapLink}`;

    /* Google Calendar link */
    (function(){
      const el = document.getElementById('addGoogleCal');
      const url = new URL("https://calendar.google.com/calendar/render");
      url.searchParams.set("action","TEMPLATE");
      url.searchParams.set("text", title);
      url.searchParams.set("dates", `${startUTC}/${endUTC}`);
      url.searchParams.set("details", details);
      url.searchParams.set("location", venueText);
      el.href = url.toString();
    })();

    /* ICS download */
    document.getElementById('downloadICS').addEventListener('click', () => {
      const ics =
`BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//Wedding Invite//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
BEGIN:VEVENT
UID:${Date.now()}@wedding
DTSTAMP:${startUTC}
DTSTART:${startUTC}
DTEND:${endUTC}
SUMMARY:${title}
DESCRIPTION:${details.replace(/\n/g,'\\n')}
LOCATION:${venueText}
END:VEVENT
END:VCALENDAR`;
      const blob = new Blob([ics], {type: 'text/calendar;charset=utf-8'});
      const url = URL.createObjectURL(blob);
      const a = document.createElement('a');
      a.href = url; a.download = 'wedding-invite.ics';
      document.body.appendChild(a); a.click(); a.remove();
      URL.revokeObjectURL(url);
    });

    /* Copy venue */
    document.getElementById('copyVenue').addEventListener('click', async () => {
      try { await navigator.clipboard.writeText(venueText); alert("Venue copied to clipboard!"); }
      catch(e){ prompt("Copy the venue address:", venueText); }
    });

    /* RSVP: WhatsApp pre-filled with both contacts */
    (function(){
      const rsvpMsg =
`Assalam-o-Alaikum,

I would like to RSVP for the wedding.

${rsvpContacts}`;
      document.getElementById('rsvpBtn').href = 'https://wa.me/?text=' + encodeURIComponent(rsvpMsg);
    })();

    /* WhatsApp Share (pre-filled) */
    (function(){
      const msg =
`Ø¨ÙØ³Ù’Ù…Ù Ø§Ù„Ù„Ù‡Ù Ø§Ù„Ø±ÙÙ‘Ø­Ù’Ù…Ù°Ù†Ù Ø§Ù„Ø±ÙÙ‘Ø­ÙÙŠÙ…Ù
With the blessings of the Holy Ahlulbayt (Ø¹)

Mr & Mrs Syed Jaffar Raza Rizvi would like to invite you to the marriage of their beloved son.

